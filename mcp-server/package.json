{
  "name": "aiquila-mcp",
  "version": "0.1.6",
  "description": "AIquila - MCP server for Nextcloud integration with Claude AI",
  "type": "module",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc --noCheck || tsc",
    "start": "node dist/index.js",
    "dev": "tsx src/index.ts",
    "test": "vitest run",
    "test:watch": "vitest",
    "lint": "eslint src/",
    "lint:fix": "eslint src/ --fix",
    "format": "prettier --write src/"
  },
  "keywords": [
    "mcp",
    "nextcloud",
    "claude",
    "ai",
    "aiquila"
  ],
  "author": "sirgorro",
  "license": "MIT",
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.25.3",
    "typescript": "^5.8.3",
    "webdav": "^5.8.0",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@types/node": "^24.10.9",
    "eslint": "^9.39.1",
    "prettier": "^3.8.1",
    "tsx": "^4.20.6",
    "typescript-eslint": "^8.54.0",
    "vitest": "^4.0.18"
  },
  "overrides": {
    "fast-xml-parser": "^5.3.4"
  }
}
